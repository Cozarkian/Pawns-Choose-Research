<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<!-- MedPod -->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>MedPod</li>
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>Defs/ResearchProjectDef[@Name="MedPodResearchProjectBase"]/modExtensions/li[@Class="PawnsChooseResearch.ResearchCategory"]</xpath>
			<value>
				<medTech>1</medTech>
			</value>
		</match>
	</Operation>
<!--Replimat-->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Replimat</li>
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>Defs/ResearchProjectDef[defName="MolecularNutrientResequencing"]/modExtensions/li[@Class="PawnsChooseResearch.ResearchCategory"]</xpath>
			<value>
				<cookingTech>1</cookingTech>
			</value>
		</match>
	</Operation>

</Patch>