<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>VGP Vegetable Garden</li>
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>Defs/ResearchProjectDef[defName="agriculture" or defName="agricultureII" or defName="agricultureIII" or defName="agricultureIV" or defName="VG_Ambrosia"]/modExtensions/li[@Class="PawnsChooseResearch.ResearchCategory"]</xpath>
			<value>
				<plantTech>1</plantTech>
			</value>
		</match>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>VGP Garden Drinks UF</li>
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>Defs/ResearchProjectDef[defName="VG_industrialfermenting" or defName="VG_PrecursorBulk"]/modExtensions/li[@Class="PawnsChooseResearch.ResearchCategory"]</xpath>
			<value>
				<cookingTech>1</cookingTech>
			</value>
		</match>
	</Operation>
</Patch>